{
    "name": "foxxie",
    "version": "5.0.0",
    "description": "A utility and moderation centered discord bot, to provide orginazation to your server.",
    "main": "./dist/Foxxie.js",
    "homepage": "https://github.com/FoxxieBot/foxxie",
    "repository": {
        "type": "git",
        "url": "https://github.com/FoxxieBot/foxxie",
        "directory": "apps/foxxie"
    },
    "keywords": [
        "foxxie",
        "discord",
        "bot"
    ],
    "scripts": {
        "test": "echo Testing",
        "start": "node .",
        "deps": "rm -f yarn.lock && rm -r -f node_modules && yarn",
        "lint": "prettier --check . && eslint src --ext mjs,js,ts",
        "format": "prettier --write . && eslint src --ext mjs,js,ts --fix",
        "changelog": "rm -f CHANGELOG.md && touch CHANGELOG.md && git cliff --prepend ./CHANGELOG.md -l -c ./cliff.toml -r ../../ --include-path 'apps/foxxie/*'",
        "build": "tsc -b",
        "clean": "rm -r node_modules && yarn",
        "watch": "tsc -b -w",
        "dev": "yarn build && yarn start",
        "watch:start": "tsc-watch -b --onSuccess \"yarn start\"",
        "clean:dist": "rm -r dist",
        "production": "NODE_ENV=production node ."
    },
    "imports": {
        "#database/*": "./dist/lib/database/*.js",
        "#database/Keys": "./dist/lib/database/Keys/index.js",
        "#external/Api": "./dist/lib/util/External/Api.js",
        "#external/GuildMemberFetchQueue": "./dist/lib/util/External/GuildMemberFetchQueue.js",
        "#external/LongLivingReactionCollector": "./dist/lib/util/External/LongLivingReactionCollector.js",
        "#external/PaginatedMessage": "./dist/lib/util/External/PaginatedMessage.js",
        "#external/Type": "./dist/lib/util/External/Type.js",
        "#lib/*": "./dist/lib/*.js",
        "#lib/audio": "./dist/lib/audio/index.js",
        "#lib/database": "./dist/lib/database/index.js",
        "#lib/discord": "./dist/lib/discord/index.js",
        "#lib/env": "./dist/lib/env/index.js",
        "#lib/FoxxieClient": "./dist/lib/FoxxieClient.js",
        "#lib/game": "./dist/lib/game/index.js",
        "#lib/i18n": "./dist/lib/i18n/index.js",
        "#lib/setup": "./dist/lib/setup/index.js",
        "#lib/structures": "./dist/lib/structures/index.js",
        "#lib/types": "./dist/lib/types/index.js",
        "#languages": "./dist/languages/index.js",
        "#root/*": "./dist/*.js",
        "#root/config": "./dist/config.js",
        "#utils/*": "./dist/lib/util/*.js",
        "#utils/API": "./dist/lib/util/API/index.js",
        "#utils/birthday": "./dist/lib/util/birthday.js",
        "#utils/constants": "./dist/lib/util/constants.js",
        "#utils/Discord": "./dist/lib/util/Discord/index.js",
        "#utils/moderation": "./dist/lib/util/moderation.js",
        "#utils/regexCreator": "./dist/lib/util/regexCreator.js",
        "#utils/transformers": "./dist/lib/util/transformers.js",
        "#utils/util": "./dist/lib/util/util.js",
        "#utils/decorators": "./dist/lib/util/decorators.js",
        "#external/*": "./dist/lib/util/External/*.js"
    },
    "funding": {
        "type": "individual",
        "url": "https://ko-fi.com/ruffpuff"
    },
    "author": "ruffpuff",
    "license": "MIT",
    "dependencies": {
        "@aero/sanitizer": "^1.3.2",
        "@favware/graphql-pokemon": "6.5.13",
        "@foxxie/fetch": "0.0.4",
        "@foxxie/lock-queue": "0.1.2",
        "@influxdata/influxdb-client": "^1.35.0",
        "@ruffpuff/utilities": "^4.0.0",
        "@sapphire/decorators": "^4.3.8",
        "@sapphire/framework": "next",
        "@sapphire/plugin-editable-commands": "^1.2.2",
        "@sapphire/plugin-i18next": "next",
        "@sapphire/plugin-logger": "next",
        "@sapphire/plugin-scheduled-tasks": "^2.3.5",
        "@sapphire/plugin-subcommands": "next",
        "@sapphire/ratelimits": "^2.4.11",
        "@sapphire/stopwatch": "^1.5.4",
        "@sapphire/utilities": "^3.18.2",
        "@sentry/node": "^6.19.7",
        "@skyra/audio": "^2.0.0",
        "@skyra/char": "^1.0.3",
        "@skyra/jaro-winkler": "^1.1.1",
        "bull": "^4.16.5",
        "canvas-constructor": "^5.0.2",
        "colorette": "^2.0.20",
        "discord.js": "^13.17.1",
        "dotenv-cra": "^3.0.3",
        "i18next": "^21.10.0",
        "ioredis": "^4.30.0",
        "lexure": "^0.17.0",
        "moment-timezone": "^0.6.0",
        "mongodb": "^3.7.4",
        "skia-canvas": "^0.9.30",
        "typeorm": "^0.2.45"
    },
    "devDependencies": {
        "@foxxie/eslint-config": "0.0.2",
        "@foxxie/types": "0.0.6",
        "@types/aero__sanitizer": "1.3.6",
        "@types/bull": "3.15.9",
        "@types/ioredis": "4.28.10",
        "@types/node": "17.0.45",
        "@types/skia-canvas": "0.9.28",
        "@types/spotify-api": "0.0.25",
        "@types/tinycolor2": "1.4.6",
        "@typescript-eslint/eslint-plugin": "5.15.0",
        "@typescript-eslint/parser": "5.15.0",
        "discord-api-types": "0.29.0",
        "eslint": "8.57.1",
        "tsc-watch": "4.6.2"
    }
}
