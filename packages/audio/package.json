{
		"name": "@foxxiebot/audio",
		"version": "0.0.2",
		"description": "A TypeScript wrapper for Lavalink.",
		"author": "Ruffpuff1",
		"main": "dist/cjs/index.cjs",
		"module": "dist/esm/index.mjs",
		"types": "dist/cjs/index.d.cts",
		"exports": {
				"import": {
						"types": "./dist/esm/index.d.mts",
						"default": "./dist/esm/index.mjs"
				},
				"require": {
						"types": "./dist/cjs/index.d.cts",
						"default": "./dist/cjs/index.cjs"
				}
		},
		"license": "MIT",
		"keywords": [
				"foxxie",
				"audio",
				"lavalink"
		],
		"files": [
				"dist/"
		],
		"engines": {
				"node": ">=v16.0.0",
				"npm": ">=7.0.0"
		},
		"publishConfig": {
				"access": "public"
		},
		"scripts": {
				"build": "yarn gen-index && tsup && yarn build:rename-cjs-index",
				"build:rename-cjs-index": "tsx ../../scripts/rename-cjs-index.cts",
				"prepack": "yarn build",
				"gen-index": "tsx ../../scripts/gen-index.cts audio --write"
		},
		"dependencies": {
				"backoff": "^2.5.0",
				"tslib": "^2.8.1",
				"ws": "^8.18.3"
		},
		"devDependencies": {
				"@commitlint/cli": "^16.3.0",
				"@commitlint/config-conventional": "^16.2.4",
				"@sapphire/eslint-config": "^4.4.3",
				"@sapphire/prettier-config": "^1.4.5",
				"@sapphire/ts-config": "^3.3.4",
				"@types/backoff": "^2.5.5",
				"@types/jest": "^27.5.2",
				"@types/node": "^17.0.45",
				"@types/ws": "^8.18.1",
				"@typescript-eslint/eslint-plugin": "^5.17.0",
				"@typescript-eslint/parser": "^5.17.0",
				"cz-conventional-changelog": "^3.3.0",
				"discord-api-types": "^0.30.0",
				"eslint": "^8.57.1",
				"eslint-config-prettier": "^8.10.2",
				"eslint-plugin-prettier": "^4.2.5",
				"gen-esm-wrapper": "^1.1.3",
				"husky": "^7.0.4",
				"jest": "^27.5.1",
				"jest-circus": "^27.5.1",
				"lint-staged": "^12.5.0",
				"prettier": "^2.8.8",
				"pretty-quick": "^3.3.1",
				"standard-version": "^9.3.2",
				"ts-jest": "^27.1.5",
				"ts-node": "^10.9.2",
				"typedoc": "^0.22.13",
				"typescript": "^5.7.2"
		}
}
