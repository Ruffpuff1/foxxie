{
  "name": "foxxie",
  "version": "8.0.0",
  "description": "Foxxie Bot",
  "main": "./dist/Foxxie.js",
  "type": "module",
  "scripts": {
    "build": "yarn prisma generate && npx tsc -b src",
    "watch": "tsc -b src -w",
    "watch:start": "tsc-watch -b src --onSuccess \"yarn start\"",
    "prisma:generate": "yarn prisma generate",
    "start": "node --enable-source-maps dist/Foxxie.js",
    "dev": "yarn build && yarn start NODE_ENV=development",
    "lint": "eslint --fix src",
    "format": "prettier --write --log-level=warn \"{src,tests}/**/*.{js,ts,json}\"",
    "start:docker": "docker-compose up --build foxxie",
    "docker-rebuild": "docker build -t ruffpuff/foxxie:latest . --no-cache",
    "update": "yarn upgrade-interactive"
  },
  "contributors": [
    "Ruffpuff1 <reeseharlak5@gmail.com>",
    "Raindrop-Droptop"
  ],
